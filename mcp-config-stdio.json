{
  "mcpServers": {
    "biothings-mcp": {
      "command": "uvx",
      "args": ["--from", "biothings-mcp", "stdio"],
      "env": {}
    },
    "gget-mcp": {
      "command": "uvx",
      "args": ["gget-mcp", "stdio"]
    },
    "synergy-age-mcp": {
      "command": "uvx",
      "args": ["synergy-age-mcp"],
      "env": {
        "MCP_TRANSPORT": "stdio"
      }
    },
    "opengenes-mcp": {
      "command": "uvx",
      "args": ["opengenes-mcp"],
      "env": {
        "MCP_TRANSPORT": "stdio"
      }
    },
    "pharmacology-mcp": {
      "command": "uvx",
      "args": ["--from", "pharmacology-mcp", "stdio"]
    }
  }
} 
